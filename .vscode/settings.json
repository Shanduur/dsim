{
    "editor.detectIndentation": true,
    "emmet.excludeLanguages": [
        "sql"
    ],
    "[json]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
    },
    "[go]": {
        "editor.tabSize": 4,
    },
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
    },
    "protoc": {
        "path": "/usr/bin/protoc",
        "options": [
            "--proto_path=pkg/proto/",
        ]
    }
}